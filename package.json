{
    "name": "@modelica/crosscheck-update",
    "version": "0.1.0",
    "description": "Code for updating the FMI cross-check data",
    "main": "lib/index.js",
    "scripts": {
        "genmod": "node genmodules.js > .gitmodules",
        "update": "git submodule init && git submodule update",
        "deinit": "git submodule deinit --force .",
        "build": "npm run genmod && npm run update",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
