    "license": "MIT",
    "bugs": {
        "url": "https://github.com/modelica/crosscheck-update/issues"
    },
    "homepage": "https://github.com/modelica/crosscheck-update#readme",
    "dependencies": {
        "@modelica/fmi-xc-scripts": "^0.4.0"
    }
}
